{
  "name": "web-template-ts",
  "description": "A TypeScript starter for static sites.",
  "version": "1.0.0",
  "private": true,
  "author": "Atelier Yoori",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "yarn type-check && yarn lint && yarn format && gatsby clean && yarn test && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "yarn jest",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
  },
  "dependencies": {
    "@fontsource/sawarabi-mincho": "^4.5.8",
    "@react-three/drei": "^9.14.3",
    "@react-three/fiber": "^8.0.27",
    "framer-motion": "^9.0.1",
    "gatsby": "^5.0.1",
    "gatsby-plugin-image": "^3.5.0",
    "gatsby-plugin-sharp": "^5.3.2",
    "gatsby-plugin-styled-components": "^6.5.0",
    "gatsby-source-contentful": "^8.5.0",
    "gatsby-transformer-sharp": "^5.3.1",
    "node": "^19.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.5",
    "three": "^0.142.0",
    "three-stdlib": "^2.12.1",
    "tsconfig-paths-webpack-plugin": "3.5.2"
  },
  "devDependencies": {
    "@babel/preset-react": "7.17.12",
    "@babel/preset-typescript": "7.17.12",
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/jest": "27.5.2",
    "@types/node": "^18.13.0",
    "@types/prettier": "^2.6.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet": "^6.1.5",
    "@types/styled-components": "^5.1.25",
    "@types/three": "^0.141.0",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "babel-jest": "28.1.1",
    "babel-preset-gatsby": "2.16.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-n": "15.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "^28.1.1",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.5",
    "typescript": "4.7.4"
  }
